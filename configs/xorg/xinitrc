#SUBTLE
nm-applet --sm-disable &

wmname LG3D &
setxkbmap -layout pt
xmodmap ~/.Xmodmap &
xrdb -merge ~/.Xdefaults &
unclutter -idle 3 &
QT_STYLE_OVERRIDE=gtk dropbox &

urxvt &
pidgin &

conky &
exec subtle



