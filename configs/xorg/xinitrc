#SUBTLE
dropboxd &
nm-applet --sm-disable &

wmname LG3D &
setxkbmap -layout pt
xmodmap ~/.Xmodmap &
xrdb -merge ~/.Xdefaults &
unclutter -idle 3 &

urxvt &
pidgin &

conky &
exec subtle 

